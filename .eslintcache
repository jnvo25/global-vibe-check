/Users/natishakhadgi/Documents/projects/global-vibe-check/src/screens/Landing.jsx",[],"/Users/natishakhadgi/Documents/projects/global-vibe-check/src/screens/CovidColorGuide.jsx",[],"/Users/natishakhadgi/Documents/projects/global-vibe-check/src/screens/CovidStats.jsx",[],"/Users/natishakhadgi/Documents/projects/global-vibe-check/src/screens/Globe.jsx",[],"/Users/natishakhadgi/Documents/projects/global-vibe-check/src/screens/AboutUs.jsx",[],"/Users/natishakhadgi/Documents/projects/global-vibe-check/src/components/Stars.jsx",[],"/Users/natishakhadgi/Documents/projects/global-vibe-check/src/components/CovidLevel.jsx",[],"/Users/natishakhadgi/Documents/projects/global-vibe-check/src/screens/AboutUs/Person.js",[],"/Users/natishakhadgi/Documents/projects/global-vibe-check/src/screens/GlobePage/InfographicModal.jsx",["57","58"],"import React from 'react';\nimport {Modal, Button, Card, ListGroupItem, ListGroup, Row, Col} from 'react-bootstrap';\nimport LoadingOverlay from 'react-loading-overlay';\n\nimport './InfographicModal.css';\n\nfunction InfographicModal(props) {\n\n    function handleClick() {\n        props.setShow(false);\n    }\n\n    function suffix(number) {\n        const lastDigit = number % 10;\n        if (lastDigit === 1) {\n            return 'st';\n        } else if (lastDigit === 2) {\n            return 'nd';\n        } else if (lastDigit === 3) {\n            return 'rd';\n        } else {\n            return 'th';\n        }\n    }\n\n    function getVibes(risk) {\n        if (risk >= 0.5) {\n            return \"horrendous\";\n        } else if (risk >= 0.15) {\n            return \"off\";\n        } else if (risk >= 0.05) {\n            return \"aight\";\n        } else {\n            return \"immaculate\";\n        }\n    }\n\n    function colorByRisk(risk) {\n        if (risk >= 0.5) {\n            return \"rgb(200, 0, 255)\";\n        } else if (risk >= 0.15) {\n            return \"rgb(255, 0, 0)\";\n        } else if (risk >= 0.05) {\n            return \"rgb(255, 165, 0)\";\n        } else {\n            return \"rgb(255, 230, 0)\";\n        }\n    }\n\n    function travelSite(country, city) {\n        if (country === \"United States of America\") {\n            country = \"usa\";\n        } else if (country === \"United Kingdom\") {\n            country = \"the-united-kingdom\";\n        }\n        return \"https://www.lonelyplanet.com/\" + country.toLowerCase().split(' ').join('-') + '/' + city.toLowerCase().split(' ').join('-');\n    }\n    return (\n        <div>\n            <Modal\n                {...props}\n                size=\"lg\"\n                aria-labelledby=\"contained-modal-title-vcenter\"\n                centered\n            >\n                <LoadingOverlay\n                    active={props.isLoading}\n                    spinner\n                    text='Loading...'\n                >\n                <Modal.Header closeButton>\n                </Modal.Header>\n                    <Card>\n                        <Card.Img variant=\"top\" className=\"card-image\" src={props.imageURL.replace(/['\"]+/g, '')} />\n                        <Card.Title style={{padding: \"20px 0px 0px 20px\", color: props.vacStats !== undefined ? colorByRisk(props.vacStats.Infection_Risk) : \"gray\"}}>{props.name}, {props.country} | Vibes are {props.vacStats !== undefined ? getVibes(props.vacStats.Infection_Risk) : \"..not found\"}</Card.Title>\n                        <Card.Body>\n\n                        </Card.Body>\n                        <ListGroup className=\"list-group-flush\">\n\n                            <ListGroupItem>Active COVID-19 cases in {props.country}: <strong>{props.covidStats.confirmed - props.covidStats.recovered - props.covidStats.deaths}</strong></ListGroupItem>\n                            <ListGroupItem>New cases (today): <strong>{props.vacStats !== undefined ? props.vacStats.NewCases: 0}</strong></ListGroupItem>\n                            <ListGroupItem>{props.country} ranked <strong>{props.countryRanking}{suffix(props.countryRanking)}</strong> in the world in active COVID-19 cases (as a percentage of the population)</ListGroupItem>\n                            <ListGroupItem></ListGroupItem>\n                        </ListGroup>\n                        <Card.Body className=\"card-links\">\n                            <Card.Link href={props.cdcUrl} target=\"_blank\">CDC Travel Recommendations</Card.Link>\n                            <Card.Link href={travelSite(props.country, props.name)} target=\"_blank\">Travel Guide</Card.Link>\n                        </Card.Body>\n                    </Card>\n                <Modal.Footer>\n                    <Button onClick={handleClick}>Close</Button>\n                </Modal.Footer>\n                </LoadingOverlay>\n            </Modal>\n        </div>\n    );\n}\n\nexport default InfographicModal;",{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","severity":1,"message":"64","line":1,"column":17,"nodeType":"65","messageId":"66","endLine":1,"endColumn":25},{"ruleId":"63","severity":1,"message":"67","line":2,"column":23,"nodeType":"65","messageId":"66","endLine":2,"endColumn":29},{"ruleId":"63","severity":1,"message":"68","line":2,"column":56,"nodeType":"65","messageId":"66","endLine":2,"endColumn":59},{"ruleId":"63","severity":1,"message":"69","line":2,"column":61,"nodeType":"65","messageId":"66","endLine":2,"endColumn":64},"no-native-reassign",["70"],"no-negated-in-lhs",["71"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","'Button' is defined but never used.","'Row' is defined but never used.","'Col' is defined but never used.","no-global-assign","no-unsafe-negation"]
